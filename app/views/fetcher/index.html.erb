<audio>
	<source src="audio/click.wav"></source>
</audio>

<div class="h-center-wrapper">
</div>
<div class="h-centered">

  <div class="w-center-wrapper">
    <div class="w-centered">

      <%= form_tag fetcher_get_user_path, :remote => true, :class => 'form', :id => 'search' do %>
        <div id="partialbox">
          <%= render :partial => 'box'  %>
          
          <%=	image_tag('label.png', :alt => 'Press enter to search!') %>
        </div>
      <% end %>
      
      <div id="partialimage">
        <%= render :partial => 'image'  %>
      </div>
    </div>
  </div>
</div>