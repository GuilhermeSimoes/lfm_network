<audio>
	<source src="audio/click.wav"></source>
</audio>

<div class="h-center-wrapper">
</div>
<div class="h-centered">

  <div class="w-center-wrapper">
    <div class="w-centered">
      <h1>User!</h1>
      <%= form_tag fetch_user_path, :remote => true, :'data-type' => 'xml', :id => 'search', :class => 'form' do %>
        <div id="partialbox">
          <%= render :partial => 'box'  %>
        </div>
      <% end %>
      
      <%=	image_tag 'ajax-loader.gif', :alt => 'Loading... Please wait.', :id => 'loader', :style => "display: none" %>
      
      <div id="partialimage">
        <%= render :partial => 'image'  %>
      </div>
    </div>
  </div>
</div>